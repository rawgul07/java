import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a=0;
        int b=1;
        System.out.print(a+" ");
        System.out.print(b+" ");
        for(int i=2;i<=n;i++){
            int n3=a+b;
            System.out.print(n3+" ");
            a=b;
            b=n3;
        }
    }
}
