import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		String str=s.nextLine();
		char ch[]=str.toCharArray();
		int flag=0;
		int i=0;
		int j=ch.length-1;
		while(i<j){
		    if(ch[i]!=ch[j]){
		        flag=1;
		        break;
		    }
		    i++;
		    j--;
		}
		if(flag==0){
		    System.out.print("palindrome");
		}
		else{
		    System.out.print("not palindrome");
		}
	}
}
